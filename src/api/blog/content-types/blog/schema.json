{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blogs",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "blogTitle": {
      "type": "string"
    },
    "blogDescription": {
      "type": "text"
    },
    "blogDate": {
      "type": "date"
    },
    "slug": {
      "type": "uid",
      "targetField": "blogTitle"
    },
    "blogContent": {
      "type": "customField",
      "options": {
        "output": "Markdown",
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
